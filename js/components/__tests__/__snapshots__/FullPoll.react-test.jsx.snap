                    <div
                      className="col-md-2 col-sm-4 col-xs-4">
                      <button
                        className="vote-button btn btn-default"
                        data-placement="bottom"
                        data-toggle="tooltip"
                        disabled={false}
                        onClick={[Function bound vote]}
                        onMouseOut={[Function bound onButtonMouseOut]}
                        onMouseOver={[Function bound onButtonMouseIn]}
                        title="Click to Vote"
                        type="button">
                        Vote
                      </button>
                    </div>
                    <div
                      className="col-md-10 col-sm-8 col-xs-8">
                      <div>
                        7:30am
                      </div>
                    </div>
                  </div>
                  <div
                    className="answer-option poll-listing uneditable row">
                    <div
                      className="col-md-2 col-sm-4 col-xs-4">
                      <button
                        className="vote-button btn btn-default"
                        data-placement="bottom"
                        data-toggle="tooltip"
                        disabled={false}
                        onClick={[Function bound vote]}
                        onMouseOut={[Function bound onButtonMouseOut]}
                        onMouseOver={[Function bound onButtonMouseIn]}
                        title="Click to Vote"
                        type="button">
                        Vote
                      </button>
                    </div>
                    <div
                      className="col-md-10 col-sm-8 col-xs-8">
                      <div>
                        8:00am
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <div
                className="new-answer-option-form container">
                <div
                  className="form-group has-feedback">
                  <div
                    className="form-group has-feedback">
                    <input
                      autoComplete="off"
                      className="form-control"
                      disabled="disabled"
                      id="newAnswerOptionField"
                      onChange={[Function bound handleNewAnswerOptionChange]}
                      placeholder="Login to add a poll answer option"
                      title="Login to add a poll answer option"
                      type="text"
                      value="" />
                  </div>
                  <div
                    className="form-group">
                    <button
                      className="btn btn-default"
                      disabled={true}
                      onClick={[Function bound handleAnswerOptionAdd]}
                      title="Login to add a poll answer option"
